      "model": "openai/gpt-oss-20b",
      "timeout_s": 60.0,
      "max_retries": 2,
      "api_key_env": "LLM_API_KEY",
      "sequential": false
    },
    "qwen3-4b-local": {
      "name": "qwen3-4b-local",
      "base_url": "http://192.168.1.8:1234",
      "endpoint": "/v1/chat/completions",
      "model": "qwen/qwen3-4b-2507",
      "timeout_s": 60.0,
      "max_retries": 2,
      "api_key_env": "LLM_API_KEY",
      "sequential": false
    },
    "qwen2-human": {
      "name": "qwen2-human",
      "base_url": "http://192.168.1.8:1234",
      "endpoint": "/v1/chat/completions",
      "model": "dpo-qwen2-1.5b-instruct-human-like",
      "timeout_s": 60.0,
      "max_retries": 2,
      "api_key_env": "LLM_API_KEY",
      "sequential": false
    },
    "qwen2-human-raw": {
      "name": "qwen2-human-raw",
      "base_url": "http://192.168.1.8:1234",
      "endpoint": "/v1/chat/completions",
      "model": "dpo-qwen2-1.5b-instruct-human-like",
      "timeout_s": 60.0,
      "max_retries": 2,
      "api_key_env": "LLM_API_KEY",
      "sequential": false,
      "enforce_json": false
    }
  },
  "registry": {
    "jd_analysis.generate_competency_matrix": "gpt-oss-20b-local",
    "rubric_design.generate_rubric": "gpt-oss-20b-local",
    "flow_manager.warmup_agent": "qwen3-4b-local",
    "flow_manager.competency_primer": "qwen3-4b-local",
    "flow_manager.competency_agent": "qwen3-4b-local",
    "candidate_agent.auto_reply": "qwen2-human",
    "flow_manager.evaluator_agent": "qwen3-4b-local",
    "flow_manager.persona_agent": "qwen2-human-raw"
  },
  "flow": {
    "warmup_questions": 1,
    "max_competency_followups": 3,
    "low_score_threshold": 2,
    "persona_enabled": true
  }
